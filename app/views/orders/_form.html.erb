<%= form_for(@order) do |f| %>
  <% if @order.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@order.errors.count, "error") %> prohibited this order from being saved:</h2>

      <ul>
      <% @order.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :round %><br />
    <%= f.number_field :round %>
  </div>
  <div class="field">
    <%= f.label :pick %><br />
    <%= f.number_field :pick %>
  </div>
  <div class="field">
    <%= f.label :team_id %><br />
    <%= f.select :team_id,@teams.collect{|t| [t.teamname,t.id]} %>
  </div>
  <div class="field">
    <%= f.label :player_id %><br />
    <%= f.select :player_id,@players.collect{|p| [p.playername,p.id]} %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
